{
  "compilerOptions": {
    "verbatimModuleSyntax": false,
    "isolatedModules": true,
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noEmitOnError": false
  }
}
