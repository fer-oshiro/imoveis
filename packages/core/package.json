{
  "name": "@imovel/core",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs"
    }
  },
  "scripts": {
    "test": "sst shell vitest",
    "build": "tsc -p tsconfig.build.json --noResolve --noCheck ",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.18",
    "sst": "*"
  },
  "devDependencies": {
    "tsdown": "^0.15.4",
    "vitest": "^3"
  }
}
